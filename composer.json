{
    "name": ":vendor/:package_name",
    "description": "A google page speed tile for the Laravel Dashboard",
    "keywords": [
        "quaterloop",
        "laravel-dashboard-google-page-speed-tile"
    ],
    "homepage": "https://github.com/quaterloop/laravel-dashboard-google-page-speed-tile",
    "license": "MIT",
    "authors": [
        {
            "name": ":Gero Wallisch",
            "email": ":gero@gerowallisch.com",
            "homepage": "https://:vendor.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^7.4",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "quaterloop\\GooglePageSpeedTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "quaterloop\\GooglePageSpeedTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "quaterloop\\GooglePageSpeedTile\\GooglePageSpeedTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
